                <% carousels.forEach(function(carousel, index) { %>
                    <div class="imgslide fade">
                        <div class="numberslide"><%= index + 1 %>/<%= carousels.length %></div>
                        <img src="<%= carousel.image %>" alt="<%= carousel.title %>">
                        <div class="text-slide"><%= carousel.title %></div>
                        <a class="prev" onclick="nextSlide(-1)">&#10094;</a>
                        <a class="next" onclick="nextSlide(1)">&#10095;</a>
                    </div>
                <% }); %>
            </div>

            <div class="page">
                <% for (var i = 1; i <= carousels.length; i++) { %>
                    <span class="dot" onclick="dotSlide(<%= i %>)"></span>
                <% } %>
            </div>
        </div>
    </div>
</section>



      





