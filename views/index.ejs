            <div class="form-box login">
                <h2>Login</h2>
                <!-- Formulir login -->
                <form action="/login" method="POST">
                    <div class="input-box">
                        <span class="icon"><ion-icon name="mail-outline"></ion-icon></span>
                        <input type="email" name="email" required>
                        <label>Email</label>
                    </div>
